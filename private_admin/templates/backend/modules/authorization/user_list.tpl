<fieldset class="admin_fieldset user_tree_fieldset">	<div class="fieldset-title">Gebruikers</div>	<div class="user_tree">		{if count($users) > 0}			<ul>				{foreach from=$users item=user}					{if $user.is_current}						<strong>					{/if}					<li class="user_list_item">						<a title="{$user.fullname}" href="/admin/index.php?user={$user.id}">{$user.fullname}</a>					</li>					{if $user.is_current}						</strong>					{/if}				{/foreach}			</ul>		{/if}	</div></fieldset>